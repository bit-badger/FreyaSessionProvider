<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <VersionPrefix>0.8.0</VersionPrefix>
    <Description>The RavenDB session store implementation for use with FreyaSessionProvider</Description>
    <PackageReleaseNotes>Initial release</PackageReleaseNotes>
    <Authors>bit-badger; danieljsummers</Authors>
    <Company>Bit Badger Solutions</Company>
    <PackageProjectUrl>https://github.com/bit-badger/FreyaSessionProvider</PackageProjectUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <RepositoryUrl>https://github.com/bit-badger/FreyaSessionProvider</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <Copyright>MIT License</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>Freya Session Cookie F# FSharp</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="RavenDBSessionStore.fs" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\FreyaSessionProvider\FreyaSessionProvider.fsproj" />
  </ItemGroup>
  <Import Project="..\.paket\Paket.Restore.targets" />
</Project>